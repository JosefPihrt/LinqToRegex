<Project>
  <Target Name="GenerateDocumentation" AfterTargets="Build" Condition=" '$(Configuration)' == 'Release'">
    <PropertyGroup>
      <RoslynatorExe>E:\Dokumenty\Visual Studio 2017\Projects\Roslynator\src\CommandLine\bin\Debug\net461\roslynator</RoslynatorExe>
    </PropertyGroup>
    <Exec Command="&quot;$(RoslynatorExe)&quot; generate-doc ^
      &quot;$(SolutionPath)&quot; ^
      --msbuild-path &quot;$(MSBuildBinPath)&quot; ^
      --projects LinqToRegex ^
      -o &quot;$(SolutionDir)..\docs\api&quot; ^
      -h &quot;LINQ to Regex API Reference&quot; ^
      -v diag" LogStandardErrorAsError="true" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="OutputOfExec" />
    </Exec>
    <Exec Command="&quot;$(RoslynatorExe)&quot; list-symbols ^
      &quot;$(SolutionPath)&quot; ^
      --msbuild-path &quot;$(MSBuildBinPath)&quot; ^
      --projects LinqToRegex ^
      --visibility public ^
      --ignored-parts containing-namespace assembly-attributes ^
      --output &quot;$(SolutionDir)..\docs\api.txt&quot; ^
      -v diag" LogStandardErrorAsError="true" ConsoleToMSBuild="true">
      <Output TaskParameter="ConsoleOutput" PropertyName="OutputOfExec" />
    </Exec>
  </Target>
</Project>